package taranet;

public class Simple04 {
	
	public static void main(String[] args){
		resever(1234);
	}
	
	public static void resever(int num) {
		//123456翻转  654321
		// 
		int sum=0;
		for(int i=num;i<=0;i/=10){
			System.out.println(i);
			int n=i%10;
			sum = sum*10+n;
			System.out.println(n);
		}
		 
	}
}
