package taranet;

public class StringToInt {

	
	public static void main(String[] args){
		String s="123";
		int tmp=0;
		for(int i=0;i<s.length();i++){
		int m=0;
		m=s.length()-(i+1);
		tmp=s.charAt(i)*(int)Math.pow(10, m);
			System.out.println(tmp);
		}
		System.out.println(tmp);
	}
}
//1    10  2
//2    10  1
//3    10  0