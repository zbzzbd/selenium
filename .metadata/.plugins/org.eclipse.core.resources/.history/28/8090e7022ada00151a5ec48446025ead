package taranet;

public class Simple04 {
	
	public static void main(String[] args){
		 int num=12345;
		 int sum=0;
		
		
		while(num>=0){
			num=num/10;
			int n = num%10;
			sum=sum*10+n;
		}
		 System.out.println(sum);
		 
	}
	
