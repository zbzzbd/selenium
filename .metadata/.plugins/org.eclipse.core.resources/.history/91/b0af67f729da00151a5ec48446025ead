package taranet;

public class Simple04 {
	
	public static void main(String[] args){
		 int num=12345;
		 int sum=0;
		 System.out.println(sum);
		for(int i=num;i<=0;i/=10){
			System.out.println(i);
			int n=i%10;
			sum = sum*10+n;
			System.out.println(n);
		}
	}
	
