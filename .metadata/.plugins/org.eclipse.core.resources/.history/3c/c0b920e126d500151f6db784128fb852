package taranet;

public class Simple02 {
	public static void main(String[] args){
		test1();
	}

	/*
	 * StringBuffer 后 可追加
	 * */
	public static void test1(){
		StringBuffer sb = new StringBuffer();
		sb.append("select *");
		sb.append("from emp");
		sb.append("where name=tom");
		String sql = sb.toString();
		System.out.println(sql);
	}
	
	
}
